$ mkdir d
$ cd d

$ whoami
> root

$ touch a b c d e f
$ richacl --set 'owner@:*::allow' a
$ richacl --set 'owner@:*::allow bin:w::allow' b
$ richacl --set 'owner@:*::allow bin:a::allow' c
$ richacl --set 'owner@:*::allow bin:wa::allow' d
$ richacl --set 'bin:a::deny owner@:*::allow bin:w::allow' e
$ richacl --set 'bin:w::deny owner@:*::allow bin:a::allow' f

$ su bin
$ echo a > a
> /bin/sh: a: Permission denied
$ echo b > b
$ echo c > c
> /bin/sh: c: Permission denied
$ echo d > d
$ echo e > e
$ echo f > f
> /bin/sh: f: Permission denied

$ echo A >> a
> /bin/sh: a: Permission denied
$ echo B >> b
> /bin/sh: b: Permission denied
$ echo C >> c
$ echo D >> d
$ echo E >> e
> /bin/sh: e: Permission denied
$ echo F >> f

$ su
$ cat a b c d e f
> b
> C
> d
> D
> e
> F

$ cd ..
$ rm -rf d
